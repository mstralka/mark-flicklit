{
  "name": "flicklit",
  "version": "1.0.0",
  "description": "A TypeScript web application for book recommendations with swipe functionality",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src scripts --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src scripts --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "import:authors": "tsx scripts/download-authors.ts",
    "import:authors:skip-download": "tsx scripts/download-authors.ts --skip-download",
    "import:authors:force": "tsx scripts/download-authors.ts --force-download",
    "import:works": "tsx scripts/download-works.ts",
    "import:works:skip-download": "tsx scripts/download-works.ts --skip-download",
    "import:works:force": "tsx scripts/download-works.ts --force-download",
    "test:recommendations": "tsx src/test-recommendations.ts",
    "test:collaborative": "tsx src/test-collaborative.ts"
  },
  "keywords": [
    "typescript",
    "vue",
    "books",
    "recommendations",
    "prisma",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.6",
    "prisma": "^6.12.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.0",
    "@prisma/client": "^6.12.0",
    "@tailwindcss/forms": "^0.5.10",
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.10",
    "zustand": "^5.0.6"
  }
}
