{
  "name": "flicklit",
  "version": "1.0.0",
  "description": "A TypeScript web application for book recommendations with swipe functionality",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"yarn dev:backend\" \"yarn dev:frontend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --exec tsx src/backend/server.ts",
    "build": "yarn build:backend && yarn build:frontend",
    "build:frontend": "tsc --project src/frontend/tsconfig.json && vite build",
    "build:backend": "tsc --project src/backend/tsconfig.json",
    "preview": "vite preview",
    "start:backend": "node dist/backend/server.js",
    "lint": "eslint src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:backend": "tsc --noEmit --project src/backend/tsconfig.json",
    "typecheck:frontend": "tsc --noEmit --project src/frontend/tsconfig.json",
    "db:generate": "cd src/backend && prisma generate",
    "db:push": "cd src/backend && prisma db push",
    "db:migrate": "cd src/backend && prisma migrate dev",
    "db:studio": "cd src/backend && prisma studio",
    "db:seed": "cd src/backend && tsx prisma/seed.ts",
    "import:authors": "tsx scripts/download-authors.ts",
    "import:authors:skip-download": "tsx scripts/download-authors.ts --skip-download",
    "import:authors:force": "tsx scripts/download-authors.ts --force-download",
    "import:works": "tsx scripts/download-works.ts",
    "import:works:skip-download": "tsx scripts/download-works.ts --skip-download",
    "import:works:force": "tsx scripts/download-works.ts --force-download",
    "import:covers": "tsx scripts/download-covers.ts",
    "import:covers:skip-download": "tsx scripts/download-covers.ts --skip-download",
    "import:covers:force": "tsx scripts/download-covers.ts --force-download",
    "test:recommendations": "cd src/backend && tsx ../test-recommendations.ts",
    "test:collaborative": "cd src/backend && tsx ../test-collaborative.ts"
  },
  "keywords": [
    "typescript",
    "vue",
    "books",
    "recommendations",
    "prisma",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/bcryptjs": "^3.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prisma": "^6.12.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.0",
    "@prisma/client": "^6.12.0",
    "@tailwindcss/forms": "^0.5.10",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^7.7.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "zod": "^4.0.10",
    "zustand": "^5.0.6"
  }
}
